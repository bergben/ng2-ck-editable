{
  "name": "ng2-ck-editable",
  "version": "0.1.12",
  "description": "Angular 2 directive to make any div editable using a CKEditor.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bergben/ng2-ck-editable.git"
  },
  "license": "MIT",
  "main": "./dist/bundles/ng2-ck-editable.umd.js",
  "module": "./dist/ng2-ck-editable.js",
  "typings": "./dist/ng2-ck-editable.d.ts",
    "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng2-ck-editable.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-ck-editable.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "author": {
    "name": "Benedikt Berger",
    "email": "benedikt.berger@live.de"
  },
  "keywords": [
    "angular",
    "angular2",
    "CKEditor",
    "editable",
    "administration"
  ],
  "dependencies": {
    "ng2-ckeditor": "^1.1.4"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "@angular/common": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "rxjs": "^5.0.0-beta.12"
  },
  "devDependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "@types/node": "^7.0.5",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rollup": "^0.41.4",
    "rxjs": "5.1.1",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "uglify-js": "^2.7.5",
    "zone.js": "^0.7.7"
  },
  "bugs": {
    "url": "https://github.com/bergben/ng2-ck-editable/issues"
  },
  "homepage": "https://github.com/bergben/ng2-ck-editable#readme"
}
